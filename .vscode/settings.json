{
    "liveServer.settings.fullReload": true,
    "liveServer.settings.host": "127.0.0.1",
    "liveServer.settings.root": "/dist",
    "editor.parameterHints": true,
    "json.format.enable": true,
    "html.suggest.html5": true,
    "html.trace.server": "verbose",
    "html.validate.scripts": true,
    "html.validate.styles": true,
    "jshint.lintHTML": true,
    "codemetrics.basics.EnabledForHTML": true,
    "files.autoSave": "onFocusChange",
    "editor.formatOnSave": true,
    "jsFileHeader.Author": "Cliff Crerar",
    "jsFileHeader.Copyright": "TQ Admin Console",
    "jsFileHeader.License": "Product of IA",
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": false,
    "liveServer.settings.CustomBrowser": "firefox",
    "html.autoClosingTags": true,
    "css.lint.duplicateProperties": "warning",
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.emptyRules": "warning",
    "css.lint.float": "warning",
    "css.validate": true,
    "git.enabled": true,
    "git.ignoreLegacyWarning": true,
    "git.ignoreLimitWarning": true,
    "prettier.singleQuote": true,
    "npm-intellisense.importQuotes": "'",
    "html.format.enable": true,
    "editor.autoIndent": true,
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "html.format.indentInnerHtml": true,
    "editor.links": true,
    "editor.matchBrackets": true,
    "editor.occurrencesHighlight": true,
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "line",
    "editor.smoothScrolling": true,
    "workbench.editor.showIcons": true,
    "editor.autoClosingBrackets": true,
    "editor.colorDecorators": true,
    "editor.cursorBlinking": "phase",
    "editor.lightbulb.enabled": true,
    "liveServer.settings.wait": 1000,
    "liveServer.settings.port": 5501,
}